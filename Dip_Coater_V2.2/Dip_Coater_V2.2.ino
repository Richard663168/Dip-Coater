//initialize dip coater for Beaker mode
void initializeBeaker(int MLT) {
  tft.setCursor(110, 225);
  tft.setTextColor(RED);
  tft.fillRect(1, 203, 319, 100, WHITE);
  tft.print("In Progress");
  myStepper2.setSpeed(700);
  myStepper2.step(25600);
  myStepper2.step(25600);
  myStepper2.step(25600);
  myStepper2.step(25600);
  delay(1000);
  myStepper2.step(-9600);
  delay(1000);
  // adjust height based on GUI
  if (MLT >= 0) {
    for (j = 1; j < MLT + 1; j++){
      myStepper2.step(-4000);
    } 
  } else {
    for (j = 1; j < abs(MLT) + 1; j++){
      myStepper2.step(4000);
    } 
  }
}

