void loop(){
  setStepperIdle();
  char key = keypad.getKey();
  String stringKey = String(key);
  String val = lastval + stringKey;
  int btnState1 = digitalRead(BUTTON_PIN1);
  int btnState2 = digitalRead(BUTTON_PIN2); 

  //receive paramters from GUI
  if (Serial.available()) {    //wait for data available
    String str = Serial.readStringUntil('O');  //read until O
    int SPEEDA = str.substring(str.indexOf('A') + 1, str.indexOf('B')).toInt();
    int Repeat = str.substring(str.indexOf('B') + 1, str.indexOf('C')).toInt();
    int timeBottom1 = str.substring(str.indexOf('C') + 1, str.indexOf('D')).toInt();
    int timeBottom2 = str.substring(str.indexOf('D') + 1, str.indexOf('E')).toInt();
    int timeBottom3 = str.substring(str.indexOf('E') + 1, str.indexOf('F')).toInt();
    int timeTop1 = str.substring(str.indexOf('F') + 1, str.indexOf('G')).toInt();
    int timeTop2 = str.substring(str.indexOf('G') + 1, str.indexOf('H')).toInt();
    int timeTop3 = str.substring(str.indexOf('H') + 1, str.indexOf('I')).toInt();
    int Solution = str.substring(str.indexOf('I') + 1, str.indexOf('J')).toInt();
    int Position = str.substring(str.indexOf('J') + 1, str.indexOf('K')).toInt();
    int SPEEDB = str.substring(str.indexOf('K') + 1, str.indexOf('L')).toInt();
    int SPEEDC = str.substring(str.indexOf('L') + 1, str.indexOf('M')).toInt();
    int TYPE = str.substring(str.indexOf('M') + 1, str.indexOf('N')).toInt();
    int SubstrateLength = str.substring(str.indexOf('N') + 1, str.indexOf('O')).toInt();
    int SPEED1 = SPEEDA/2;
    int SPEED2 = SPEEDB/2;
    int SPEED3 = SPEEDC/2;
    int MULT = Position/5;
    displayRefresh();

    if (Solution == 1 && TYPE == 1){
      initializeBeaker(MULT);
      for(i = 1; i < Repeat + 1; i++){
        tft.setCursor(270, 194);
        tft.fillRect(270, 175, 50, 25, WHITE);
        tft.setTextColor(RED);
        tft.print(i);
        //solution 1
        dipBeakerVertical (SPEED1, SubstrateLength, timeBottom1, timeTop1);
      }
      displayRefresh();
    }
    
    if (Solution == 2 && TYPE == 1){
      initializeBeaker(MULT);
      for(i = 1; i < Repeat + 1; i++){
        tft.setCursor(270, 194);
        tft.fillRect(270, 175, 50, 25, WHITE);
        tft.setTextColor(RED);
        tft.print(i);
        //solution 1
        dipBeakerVertical(SPEED1, SubstrateLength, timeBottom1, timeTop1);
        rotate(4266);
        //solution 2
        dipBeakerVertical(SPEED2, SubstrateLength, timeBottom2, timeTop2);
        rotate(-4266);
      }
      displayRefresh();
    }  

    if (Solution == 3 && TYPE == 1){
      initializeBeaker(MULT);
      for(i = 1; i < Repeat + 1; i++){
        tft.setCursor(270, 194);
        tft.fillRect(270, 175, 50, 25, WHITE);
        tft.setTextColor(RED);
        tft.print(i);
        //solution 1
        dipBeakerVertical(SPEED1, SubstrateLength, timeBottom1, timeTop1);
        rotate(4266);
        //solution 2
        dipBeakerVertical(SPEED2, SubstrateLength, timeBottom2, timeTop2);
        rotate(4267);
        //solution 3
        dipBeakerVertical(SPEED3, SubstrateLength, timeBottom3, timeTop3);
        rotate(4267);
      }
      displayRefresh();
    } 

    if (Solution == 1 && TYPE == 2){
      initializeTube(MULT);
      for(i = 1; i < Repeat + 1; i++){
        tft.setCursor(270, 194);
        tft.fillRect(270, 175, 50, 25, WHITE);
        tft.setTextColor(RED);
        tft.print(i);
        //solution 1
        dipTubeVertical (SPEED1, timeBottom1, timeTop1);
      }
      displayRefresh();
    }

    if (Solution == 2 && TYPE == 2){
      initializeTube(MULT);
      for(i = 1; i < Repeat + 1; i++){
        tft.setCursor(270, 194);
        tft.fillRect(270, 175, 50, 25, WHITE);
        tft.setTextColor(RED);
        tft.print(i);
        //solution 1
        dipTubeVertical(SPEED1, timeBottom1, timeTop1);
        rotate(4266);
        //solution 2
        dipTubeVertical(SPEED2, timeBottom2, timeTop2);
        rotate(-4266);
      }
      displayRefresh();
    }  

    if (Solution == 3 && TYPE == 2){
      initializeTube(MULT);
      for(i = 1; i < Repeat + 1; i++){
        tft.setCursor(270, 194);
        tft.fillRect(270, 175, 50, 25, WHITE);
        tft.setTextColor(RED);
        tft.print(i);
        //solution 1
        dipTubeVertical(SPEED1, timeBottom1, timeTop1);
        rotate(4266);
        //solution 2
        dipTubeVertical(SPEED2, timeBottom2, timeTop2);
        rotate(4267);
        //solution 3
        dipTubeVertical(SPEED3, timeBottom3, timeTop3);
        rotate(4267);
      }
      displayRefresh();
    }
  }

  // setup keypad for adjust platform rotation                                          
  if(key == '*'){
    lastval = "";
    displayRefresh();
  }
  
  if (key == '0' || key == '1' || key == '2' || key == '3' || key == '4' || key == '5' || key == '6' || key == '7' || key == '8' || key == '9'){
    tft.fillScreen(WHITE);
    tft.setTextSize(2);
    tft.setTextColor(BLACK);
    tft.setCursor(130, 100);
    tft.print(val);
    lastval = val;
  }

  if(key == '#'){
    myStepper1.setSpeed(60);
    myStepper1.step(round(lastval.toInt()*12800/360));
    lastval = "";
    displayRefresh();
  }
}
