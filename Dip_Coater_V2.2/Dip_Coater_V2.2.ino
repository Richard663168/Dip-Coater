//dipcoating process for Beaker mode
void dipBeakerVertical(int SPD, int SL, int TB, int TT){
  myStepper2.setSpeed(700);
  myStepper2.step(-25600);
  myStepper2.step(-25600);
  myStepper2.step(-19600);
  myStepper2.setSpeed(300);
  myStepper2.step(-20800);
  myStepper2.setSpeed(SPD);
  //adjust for substrate size
  for (k = 1; k < SL - 15 + 1; k++){
     myStepper2.step(-1400);
  }
  setStepperIdle();
  delay(TB*1000);
  for (k = 1; k < SL - 15 + 1; k++){
     myStepper2.step(1400);
  }
  myStepper2.step(20800);
  myStepper2.setSpeed(700);
  myStepper2.step(19600);
  myStepper2.step(25600);
  myStepper2.step(25600);
  setStepperIdle();
  delay(TT*1000);
}

//dipcoating process for Tube mode
void dipTubeVertical(int SPD, int TB, int TT){
  myStepper2.setSpeed(SPD);
  myStepper2.step(-25600);
  myStepper2.step(-25600);
  myStepper2.step(-25600);
  myStepper2.step(-20800);
  setStepperIdle();
  delay(TB*1000);
  myStepper2.step(25600);
  myStepper2.step(25600);
  myStepper2.step(25600);
  myStepper2.step(20800);
  setStepperIdle();
  delay(TT*1000);
} 

//rotate platform
void rotate(int STP){
  myStepper1.setSpeed(40);
  myStepper1.step(STP);
  setStepperIdle();
  delay(50);
} 

